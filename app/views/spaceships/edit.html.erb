<h1>Edit <%= @spaceship.name %></h1>
  <%= simple_form_for @spaceship do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :photo, as: :file %>
    <%= f.input :unit_price %>
    <%= f.input :size %>
    <%= f.input :max_speed %>
    <%= f.input :capacity %>
    <%= f.submit "Edit spaceship", class: "btn btn-primary" %>
  <% end %>
</div>
